<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="p1.Register" table="user_details">
        <id name="id">
            <generator class="identity"></generator>
        </id>
        <property name="uname"></property>
        <property name="password"></property>
        <property name="email"></property>
    </class>
    <class name="p1.Certify">
        <id name="id">
            <generator class="identity"></generator>
        </id>
        <property name="b_name"></property>
        <property name="p_name"></property>
        <property name="email"></property>
        <property name="ph_no"></property>
        <property name="category"></property>
    </class>

  

</hibernate-mapping>
